<!DOCTYPE html>
<html>
	<head>
	<title>
		Zimride
	</title>	

	<%= stylesheet_link_tag 'bootstrap.min', :media => 'screen'%>
	<%= stylesheet_link_tag 'style', :media => 'screen'%>
	
	<script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
	
	<%= javascript_include_tag :all %>
	
	<%= stylesheet_link_tag 'default', :media => 'screen'%>
	<script src="https://maps.googleapis.com/maps/api/js?sensor=false&libraries=places"></script>	
	<script type="text/javascript">
		function initialize() {
	    var input = document.getElementById('searchTextField');
	    var input2 = document.getElementById('searchTextField2');
	    var input3 = document.getElementById('searchTextField3');
	    var input4 = document.getElementById('searchTextField4');
	    var options = {componentRestrictions: {country: 'in'}};
	                 
	    if (input)
	   	{ new google.maps.places.Autocomplete(input, options);}
	   	if (input2)
	    {new google.maps.places.Autocomplete(input2, options);}
		if (input3)
	   	{ new google.maps.places.Autocomplete(input3, options);}
	   	if (input4)
	   	{ new google.maps.places.Autocomplete(input4, options);}
		}
	             
		google.maps.event.addDomListener(window, 'load', initialize);
	</script>
	</head>

<body>
<div class="container_no_boostrap">
<%= render 'layouts/navbar'  %>
	<div id="flash_messages">
	
  		<% flash.each do |key, value| %>
  			<div class="alert"><p id="alert_msg"><%= value %></p>
    		
    		<button type="button" class="close" data-dismiss="alert">&times;</button>
    		</div>
  		<% end %>
		
		</div>
		
	<%= yield %>

</div>


  </body>
</html>